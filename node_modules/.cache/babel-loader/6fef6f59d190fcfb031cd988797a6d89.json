                <h1 class=\"text-muted text-center my-1\" style=\"font-weight:800\">{{ quantInfoAguardo }}</h1>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"col-md-4 px-4\">\r\n                      <div class=\"row\">\r\n                        <div class=\"col-md-10 bg-white mx-auto border-top border-right border-left rounded-top\">\r\n                          <p class=\"my-1 text-center fw-300\">Finalizadas</p>\r\n                        </div>\r\n                        <div class=\"col-md-10 bg-light mx-auto\">\r\n                          <h1 class=\"text-muted text-center my-1\" style=\"font-weight:800\">{{ quantInfoFinalizada }}</h1>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              <div class=\"col-md-4 pt-1 d-flex flex-column justify-content-center\">\r\n          <div class=\"row\">\r\n            <div class=\"col-md-12 d-flex justify-content-center align-items-center flex-row\"><router-link :to=\"{name: 'StatusBS', params: { id: this.idbusiness, card: this.id }}\" class=\"logbtn btn btn-secondary w-75 mt-3 mb-2 py-2 fw-200\" href=\"#\" style=\"\">Reportar Status</router-link></div>\r\n          </div>\r\n        <div class=\"row\">\r\n          <div class=\"col-md-12 d-flex flex-row justify-content-center align-items-center\"><a class=\"btn btn-dark w-75 py-2 mt-3 mb-3 fw-200\" href=\"#\">Acessar Resultados</a></div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { auth, db } from '../../firebase'\r\n\r\nexport default {\r\n  name: 'CardResult',\r\n  props: {\r\n    data: { type: Object, required: true }\r\n  },\r\n  data () {\r\n    return {\r\n      id: this.data.id,\r\n      iduser: null,\r\n      idbusiness: this.data.idbusiness,\r\n      infoForm: []\r\n    }\r\n  },\r\n  created () {\r\n    var user = auth.currentUser\r\n    if (user != null) {\r\n      this.iduser = user.uid\r\n    }\r\n    this.getInfo()\r\n  },\r\n  computed: {\r\n    /* eslint-disable */\r\n    quantInfoForm: function () {\r\n      if (this.infoForm !== null) {\r\n        const quantidade = this.infoForm.length\r\n        return quantidade\r\n      }\r\n    },\r\n    quantInfoAguardo: function () {\r\n      if (this.infoFormAguardo !== null) {\r\n        const quant = this.infoForm\r\n          .filter(index => index.status == 'aguardo')\r\n          return quant.length\r\n      }\r\n    },\r\n    quantInfoFinalizada: function () {\r\n      if (this.infoFormAguardo !== null) {\r\n        const quant = this.infoForm\r\n          .filter(index => index.status == 'finalizada')\r\n          return quant.length\r\n      }\r\n    }\r\n    /* eslint-enable */\r\n  },\r\n  methods: {\r\n    getInfo () {\r\n      const reft = db.collection('business').doc(this.idbusiness).collection('Cards').doc(this.id).collection('Forms').get()\r\n      reft.then(snapshot => {\r\n        snapshot.forEach(doc => {\r\n          this.infoForm.push(doc.data())\r\n        })\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n"],"sourceRoot":"src/components/business"}]}