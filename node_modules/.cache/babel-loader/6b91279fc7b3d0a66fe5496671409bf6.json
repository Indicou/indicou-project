ground-image:linear-gradient(rgb(84 255 154 / 75%), rgb(242 255 248 / 10%));background-position:top left;background-size: 100%;background-repeat: repeat;\">\r\n        <div class=\"container\">\r\n          <div class=\"row\">\r\n            <div class=\"col-md-12\">\r\n              <form class=\"form-inline\" style=\"\">\r\n                <div class=\"input-group w-100\">\r\n                  <input type=\"text\" v-model=\"busca\" class=\"busca form-control pl-3 border-left-0 border-bottom\" id=\"inlineFormInputGroup\" style=\"border-color:#e7e7e7;border-bottom-width:20px;border-top-width: 0px;box-shadow: 0px 0px #333;height: 70px;\" placeholder=\"Estou procurando por...\">\r\n                  <div class=\"input-group-append\" style=\"\">\r\n                    <button class=\"btn btn-secondary px-3\" type=\"button\" style=\"background-image:linear-gradient(rgb(255 255 255), rgb(255 255 255));background-position: top left;background-size: 100%;background-repeat: repeat;border-color: rgb(231 231 231);\"><i class=\"fa fa-search\" style=\"color: #7f7f7f;\"></i>&nbsp;<span style=\"font-weight:700\" class=\"\"></span>\r\n                    </button>\r\n                  </div>\r\n                </div>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n            <div class=\"container mt-4 px-5\">\r\n          <div class=\"row\">\r\n            <div class=\"col-md-6\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-3 text-center\">\r\n                  <button type=\"button\" class=\"btn btn-white btn-circle btn-xl btn-light text-center\"  :class=\"{active: imo}\" @click=\"imoveisb()\"><i class=\"fa fa-home fa-lg\"></i></button>\r\n                  <p class=\"text-center pt-1 text-dark txt-customer\">Imóveis</p>\r\n                </div>\r\n                <div class=\"col-md-3 text-center\">\r\n                  <button type=\"button\" class=\"btn btn-white btn-circle btn-xl btn-light text-center\" :class=\"{active: vei}\" @click=\"veiculosb()\"><i class=\"fa fa-car fa-lg\"></i></button>\r\n                  <p class=\"txt-customer ext-center pt-1 text-dark\">Veículos</p>\r\n                </div>\r\n                <div class=\"col-md-3 text-center\">\r\n                  <button type=\"button\" class=\"btn btn-white btn-circle btn-xl btn-light text-center\"  :class=\"{active: pro}\" @click=\"prodfisicob()\"><i class=\"fa fa-cubes fa-lg\"></i></button>\r\n                  <p class=\"txt-customer text-center pt-1 text-dark\">Produtos Físicos</p>\r\n                </div>\r\n                <div class=\"col-md-3 text-center\">\r\n                  <button type=\"button\" class=\"btn btn-white btn-circle btn-xl btn-light text-center\"  :class=\"{active: dig}\" @click=\"digitaisb()\"><i class=\"fa fa-home fa-laptop fa-lg\"></i></button>\r\n                  <p class=\"txt-customer text-center pt-1 text-dark\">Serviços Digitais</p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-md-6\">\r\n              <div class=\"row\">\r\n                <div class=\"text-center col-md-3\">\r\n                  <button type=\"button\" class=\"btn btn-white btn-circle btn-xl btn-light text-center\" :class=\"{active: ser}\" @click=\"servfisicob()\"><i class=\"fa fa-briefcase fa-lg\"></i></button>\r\n                  <p class=\"txt-customer text-center pt-1 text-dark\">Serviços Físicos<br></p>\r\n                </div>\r\n                <div class=\"text-center col-md-3\">\r\n                  <button type=\"button\" class=\"btn btn-white btn-circle btn-xl btn-light text-center\"  :class=\"{active: emp}\" @click=\"empresab()\"><i class=\"fa fa-building fa-lg\"></i></button>\r\n                  <p class=\"txt-customer text-center pt-1 text-dark\">Empresas<br></p>\r\n                </div>\r\n                <div class=\"text-center col-md-3\">\r\n                  <button type=\"button\" class=\"btn btn-white btn-circle btn-xl btn-light text-center\"  :class=\"{active: ap}\" @click=\"appb()\"><i class=\"fa fa-mobile fa-lg\"></i></button>\r\n                  <p class=\"txt-customer text-center pt-1 text-dark\">Apps/Softwares<br></p>\r\n                </div>\r\n                <div class=\"text-center col-md-3\">\r\n                  <button type=\"button\" class=\"btn btn-white btn-circle btn-xl btn-light text-center\" :class=\"{active: tod}\" @click=\"todosb()\"><i class=\"fa fa-th-large fa-lg\"></i></button>\r\n                  <p class=\"txt-customer text-center pt-1 text-dark\">Todos<br></p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    <modal-product/>\r\n    <div class=\"pt-3 pb-5\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <!-- <nav-left/> -->\r\n          <div class=\"col-md-12 col-lg-12\">\r\n          <div class=\"text-center pb-4\">\r\n            <div class=\"container\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-2 text-left pt-3\"><i class=\"fas d-inline fa-lg fa-grip-horizontal text-muted\"></i></div>\r\n                <div class=\"col-md-6\"></div>\r\n                <div class=\"col-md-2 px-0 pr-1\">\r\n                  <p class=\"py-1 text-right\">Ordenar por:</p>\r\n                </div>\r\n                <div class=\"col-md-2 px-0\">\r\n                  <select class=\"form-control\" v-model=\"value\">\r\n                    <option>Menor Preço</option>\r\n                    <option>Maior Preço</option>\r\n                  </select>\r\n                </div>\r\n              </div>\r\n              <!-- Aqui começa com 16 itens-->\r\n              <div class=\"row\">\r\n                <card-product\r\n                :key=\"item\"\r\n                :data=\"index\"\r\n                v-for=\"(index, item) in pageOfItems\"/>\r\n              </div>\r\n              <div class=\"justify-content-center mt-4\">\r\n                <jw-pagination :items=\"Valores\" :pageSize=8 @changePage=\"onChangePage\" :labels=\"customLabels\" ></jw-pagination>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport { db } from '../firebase'\r\nimport ModalProduct from '../components/market/ModalProduct'\r\n/* import NavLeft from '../components/market/NavLeft.vue' */\r\nimport CardProduct from '../components/market/CardProduct.vue'\r\n\r\nconst customLabels = {\r\n  first: 'Primeira',\r\n  last: 'Última',\r\n  previous: 'Anterior',\r\n  next: 'Próxima'\r\n}\r\n\r\nexport default {\r\n  name: 'Market',\r\n  components: {\r\n    /* NavLeft, */\r\n    CardProduct,\r\n    ModalProduct\r\n  },\r\n  data () {\r\n    return {\r\n      indicacoes: [],\r\n      imo: false,\r\n      pro: false,\r\n      ser: false,\r\n      vei: false,\r\n      dig: false,\r\n      emp: false,\r\n      ap: false,\r\n      tod: true,\r\n      busca: '',\r\n      pageOfItems: [],\r\n      customLabels,\r\n      filtro: '',\r\n      value: 'Menor Preço'\r\n    }\r\n  },\r\n  computed: {\r\n    Buscatodos: function () {\r\n      if (this.busca === '' || this.busca === ' ' || this.busca === null) {\r\n        if (this.filtro !== '') {\r\n          return this.indicacoes\r\n            .filter(index => index.filtro === this.filtro)\r\n        } else {\r\n          return this.indicacoes\r\n        }\r\n      } else {\r\n        return this.indicacoes\r\n          .filter(index => {\r\n            return (\r\n              this.busca.toLowerCase().split(' ').every(v =>\r\n                index.titulo.toLowerCase().includes(v) ||\r\n                index.empresa.toLowerCase().includes(v)\r\n              )\r\n            )\r\n          })\r\n      }\r\n    },\r\n    /* eslint-disable */\r\n    Valores: function () {\r\n      if (this.value === 'Menor Preço') {\r\n        return this.Buscatodos.sort(function (a, b) {\r\n        return a.valor - b.valor\r\n      })\r\n      } else {\r\n        return this.Buscatodos.sort(function (a, b) {\r\n        return b.valor - a.valor\r\n      })\r\n      }\r\n    }\r\n    /* eslint-enable */\r\n  },\r\n  created () {\r\n    this.$root.$emit('MenuBar::show')\r\n    this.$root.$emit('marketset:show')\r\n    this.$root.$emit('indset:hide')\r\n    this.$root.$emit('Business::hide')\r\n    this.getData()\r\n  },\r\n  methods: {\r\n    onChangePage (pageOfItems) {\r\n      this.pageOfItems = pageOfItems\r\n    },\r\n    mostraMais () {\r\n      this.cont = this.cont + 1\r\n      if (this.mostra16 === true) {\r\n        this.mostra16 = false\r\n        this.getData32()\r\n        this.mostra32 = true\r\n        this.cont = this.cont + 1\r\n      } if (this.motra32 === true && this.cont === 3) {\r\n        this.mostra32 = false\r\n        this.mostra64 = true\r\n        this.getData64()\r\n        this.cont = this.cont + 1\r\n      }\r\n    },\r\n    getData () {\r\n      const reft = db.collection('mercado').get()\r\n      reft.then(snapshot => {\r\n        snapshot.forEach(doc => {\r\n          this.indicacoes.push(doc.data())\r\n        })\r\n      })\r\n    },\r\n    imoveisb () {\r\n      this.filtro = 'imoveis'\r\n      this.tod = false\r\n      this.imo = true\r\n      this.pro = false\r\n      this.ser = false\r\n      this.dig = false\r\n      this.emp = false\r\n      this.ap = false\r\n      this.vei = false\r\n    },\r\n    veiculosb () {\r\n      this.filtro = 'veiculos'\r\n      this.vei = true\r\n      this.tod = false\r\n      this.imo = false\r\n      this.pro = false\r\n      this.ser = false\r\n      this.dig = false\r\n      this.emp = false\r\n      this.ap = false\r\n    },\r\n    prodfisicob () {\r\n      this.filtro = 'prodfisico'\r\n      this.vei = false\r\n      this.tod = false\r\n      this.imo = false\r\n      this.pro = true\r\n      this.ser = false\r\n      this.dig = false\r\n      this.emp = false\r\n      this.ap = false\r\n    },\r\n    digitaisb () {\r\n      this.filtro = 'digital'\r\n      this.vei = false\r\n      this.tod = false\r\n      this.imo = false\r\n      this.pro = false\r\n      this.ser = false\r\n      this.dig = true\r\n      this.emp = false\r\n      this.ap = false\r\n    },\r\n    servfisicob () {\r\n      this.filtro = 'servfisico'\r\n      this.vei = false\r\n      this.tod = false\r\n      this.imo = false\r\n      this.pro = false\r\n      this.ser = true\r\n      this.dig = false\r\n      this.emp = false\r\n      this.ap = false\r\n    },\r\n    empresab () {\r\n      this.filtro = 'empresa'\r\n      this.vei = false\r\n      this.tod = false\r\n      this.imo = false\r\n      this.pro = false\r\n      this.ser = false\r\n      this.dig = false\r\n      this.emp = true\r\n      this.ap = false\r\n    },\r\n    appb () {\r\n      this.filtro = 'app'\r\n      this.vei = false\r\n      this.tod = false\r\n      this.imo = false\r\n      this.pro = false\r\n      this.ser = false\r\n      this.dig = false\r\n      this.emp = false\r\n      this.ap = true\r\n    },\r\n    todosb () {\r\n      this.filtro = ''\r\n      this.vei = false\r\n      this.tod = true\r\n      this.imo = false\r\n      this.pro = false\r\n      this.ser = false\r\n      this.dig = false\r\n      this.emp = false\r\n      this.ap = false\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.txt-customer{\r\n  font-size:12px!important;\r\n  font-family: 'Avenir', 'Poppins', sans-serif;\r\n}\r\n.btn-circle {\r\n      width: 30px;\r\n      height: 30px;\r\n      text-align: center;\r\n      padding: 6px 0;\r\n      font-size: 12px;\r\n      line-height: 1.428571429;\r\n      border-radius: 15px;\r\n    }\r\n\r\n    .btn-circle.btn-lg {\r\n      width: 50px;\r\n      height: 50px;\r\n      padding: 10px 16px;\r\n      font-size: 18px;\r\n      line-height: 1.33;\r\n      border-radius: 25px;\r\n    }\r\n\r\n    .btn-circle.btn-xl {\r\n      width: 70px;\r\n      height: 70px;\r\n      padding: 10px 16px;\r\n      font-size: 24px;\r\n      line-height: 1.33;\r\n      border-radius: 35px;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/views"}]}