ontent":["<template>\r\n<transition name=\"modal-fade\">\r\n  <div class=\"modal-backdrop\">\r\n    <div class=\"modall\" :class=\"{altura: valid_password}\">\r\n      <form @submit.prevent=\"registerUser()\" id=18>\r\n      <header class=\"modal-header\">\r\n        <slot name=\"header\" >\r\n          <span class=\"cadastro\">Cadastro</span>\r\n        </slot>\r\n        <button\r\n          type=\"button\"\r\n          class=\"btn-close\"\r\n          @click=\"CloseRegister()\"\r\n        >\r\n          x\r\n        </button>\r\n      </header>\r\n\r\n      <section class=\"modal-body mt-1\">\r\n        <div class=\"row\">\r\n        <div class=\"form-group col-12\">\r\n          <input\r\n          required\r\n          type=\"text\"\r\n          v-model.trim=\"userData.userid\"\r\n          class=\"form-control\"\r\n          placeholder=\"Usuário\">\r\n        </div>\r\n        <div class=\"form-group col-12\">\r\n          <input\r\n          required\r\n          type=\"email\"\r\n          v-model.trim=\"userData.email\"\r\n          class=\"form-control\"\r\n          placeholder=\"E-mail\">\r\n        </div>\r\n        <div class=\"form-group col-12\">\r\n          <input\r\n          autocomplete=\"off\"\r\n          @input=\"checkPassword\"\r\n          required\r\n          type=\"password\"\r\n          v-model.trim=\"password\"\r\n          class=\"form-control\"\r\n          placeholder=\"Senha\">\r\n        </div>\r\n        <div class=\"form-group col-12\">\r\n          <input\r\n          @input=\"checkPassword\"\r\n          autocomplete=\"off\"\r\n          required\r\n          type=\"password\"\r\n          v-model.trim=\"password2\"\r\n          class=\"form-control\"\r\n          placeholder=\"Confirme sua Senha\">\r\n        </div>\r\n        <div class=\"input_container\" v-show=\"confirm\">\r\n            <ul class=\"mb-1\" style=\"font-size: 15px\">\r\n              <li class=\"lista mb-2\">A Senha deve conter:</li>\r\n              <li