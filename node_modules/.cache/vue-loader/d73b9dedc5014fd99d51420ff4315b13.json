 class=\"modal-body\">\r\n      <div class=\"card-body\">\r\n        <div class=\"form-group\">\r\n          <input\r\n          required\r\n          type=\"text\"\r\n          v-model.trim=\"userData.userid\"\r\n          class=\"form-control\"\r\n          placeholder=\"Como quer ser chamado?\">\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <input\r\n          required\r\n          type=\"email\"\r\n          v-model.trim=\"userData.email\"\r\n          class=\"form-control\"\r\n          placeholder=\"E-mail\">\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <input\r\n          required\r\n          type=\"password\"\r\n          v-model.trim=\"userData.password\"\r\n          class=\"form-control\"\r\n          placeholder=\"Senha\">\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <input\r\n          required\r\n          type=\"password\"\r\n          v-model.trim=\"userData.password2\"\r\n          class=\"form-control\"\r\n          placeholder=\"Confirme sua Senha\">\r\n        </div>\r\n      </div>\r\n      <div>\r\n        <button class=\"btn btn-danger w-100 custom-button\" :disabled=\"loading\">\r\n          <template v-if=\"loading\">\r\n            Cadastrando... <i class=\"fas fa-spinner fa-spin\"></i>\r\n          </template>\r\n          <template v-else>\r\n            Confirmar <i class=\"fas fa-sign-in-alt\"></i>\r\n          </template>\r\n        </button>\r\n      </div>\r\n      </div>\r\n    </div>\r\n    </div>\r\n  </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { db, auth } from '../../firebase'\r\nexport default {\r\n  name: 'Register',\r\n  data () {\r\n    return {\r\n      userData: {\r\n        userid: '',\r\n        email: '',\r\n        password: '',\r\n        password2: ''\r\n      },\r\n      successMessage: '',\r\n      errorMessage: '',\r\n      showRegisterModal: false,\r\n      loading: false\r\n    }\r\n  },\r\n  created () {\r\n    this.$root.$on('ModalRegister::show', () => {\r\n      this.showRegisterModal = true\r\n    })\r\n  },\r\n  methods: {\r\n    closeModal () {\r\n      this.showRegisterModal = false\r\n    },\r\n    registerUser () {\r\n      if (this.password === this.password2) {\r\n        console.log('correto')\r\n      }\r\n      auth.createUserWithEmailAndPassword(this.userData.email, this.userData.password)\r\n        .then(() => {\r\n          auth.currentUser.updateProfile({\r\n            displayName: this.userData.username\r\n          }).then(() => {\r\n            db.collection('users').add({\r\n              email: this.userData.email,\r\n              userid: this.userData.userid\r\n            }).then(() => {\r\n              this.$router.replace('MyIndications')\r\n            }).cath(err => {\r\n              this.errorMessage = err.message\r\n            })\r\n          }).cath(err => {\r\n            this.errorMessage = err.message\r\n          })\r\n        }).cath(err => {\r\n          this.errorMessage = err.message\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n"]}]}