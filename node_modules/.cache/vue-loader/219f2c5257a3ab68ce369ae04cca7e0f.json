button\r\n          type=\"button\"\r\n          class=\"btn-close\"\r\n          @click=\"close\"\r\n        >\r\n          x\r\n        </button>\r\n      </header>\r\n\r\n      <section class=\"modal-body\">\r\n        <slot name=\"body\">\r\n          This is the default body!\r\n        </slot>\r\n       </section>\r\n\r\n      <footer class=\"modal-footer\">\r\n        <slot name=\"footer\">\r\n          This is the default footer!\r\n        </slot>\r\n        <button\r\n          type=\"button\"\r\n          class=\"btn-green\"\r\n          @click=\"close\"\r\n        >\r\n          Close Modal\r\n        </button>\r\n      </footer>\r\n    </div>\r\n  </div>\r\n      </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { db, auth } from '../../firebase'\r\nexport default {\r\n  name: 'Register',\r\n  data () {\r\n    return {\r\n      userData: {\r\n        email: '',\r\n        userid: '',\r\n        password: ''\r\n      },\r\n      successMessage: '',\r\n      errorMessage: ''\r\n    }\r\n  },\r\n  methods: {\r\n    registerUser () {\r\n      auth.createUserWithEmailAndPassword(this.userData.email, this.userData.password)\r\n        .then(() => {\r\n          auth.currentUser.updateProfile({\r\n            displayName: this.userData.username\r\n          }).then(() => {\r\n            db.collection('users').add({\r\n              userid: this.userData.userid,\r\n              email: this.userData.email\r\n            }).then(() => {\r\n              this.$router.replace('MyIndications')\r\n            }).cath(err => {\r\n              this.errorMessage = err.message\r\n            })\r\n          }).cath(err => {\r\n            this.errorMessage = err.message\r\n          })\r\n        }).cath(err => {\r\n          this.errorMessage = err.message\r\n        })\r\n    },\r\n    close () {\r\n      this.$emit('close')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.modal-backdrop {\r\n    position: fixed;\r\n    top: 0;\r\n    bottom: 0;\r\n    left: 0;\r\n    right: 0;\r\n    background-color: rgba(0, 0, 0, 0.3);\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n\r\n  .modal {\r\n    background: #FFFFFF;\r\n    box-shadow: 2px 2px 20px 1px;\r\n    overflow-x: auto;\r\n    display: flex;\r\n    flex-direction: column;\r\n  }\r\n\r\n  .modal-header,\r\n  .modal-footer {\r\n    padding: 15px;\r\n    display: flex;\r\n  }\r\n\r\n  .modal-header {\r\n    position: relative;\r\n    border-bottom: 1px solid #eeeeee;\r\n    color: #4AAE9B;\r\n    justify-content: space-between;\r\n  }\r\n\r\n  .modal-footer {\r\n    border-top: 1px solid #eeeeee;\r\n    flex-direction: column;\r\n    justify-content: flex-end;\r\n  }\r\n\r\n  .modal-body {\r\n    position: relative;\r\n    padding: 20px 10px;\r\n  }\r\n\r\n  .btn-close {\r\n    position: absolute;\r\n    top: 0;\r\n    right: 0;\r\n    border: none;\r\n    font-size: 20px;\r\n    padding: 10px;\r\n    cursor: pointer;\r\n    font-weight: bold;\r\n    color: #4AAE9B;\r\n    background: transparent;\r\n  }\r\n\r\n  .btn-green {\r\n    color: white;\r\n    background: #4AAE9B;\r\n    border: 1px solid #4AAE9B;\r\n    border-radius: 2px;\r\n  }\r\n</style>\r\n"]}]}