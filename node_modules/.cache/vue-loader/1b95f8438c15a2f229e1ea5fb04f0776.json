enor Preço</button>\r\n                    <div class=\"dropdown-menu dp-menu\" x-placement=\"bottom-start\" > <a class=\"dropdown-item\" href=\"#\">Action</a>\r\n                      <div class=\"dropdown-divider\"></div>\r\n                      <a class=\"dropdown-item\" href=\"#\">Separated link</a>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <!-- Aqui começa com 16 itens-->\r\n              <div class=\"row\" v-show=\"mostra16\">\r\n                <card-product\r\n                v-show=\"mostratodos\"\r\n                :key=\"index\"\r\n                :data=\"item\"\r\n                v-for=\"(item, index) in Buscatodos16\"/>\r\n              </div>\r\n              <!-- Aqui começa com 32 itens-->\r\n              <div class=\"row\" v-show=\"mostra32\">\r\n                <card-product\r\n                v-show=\"mostratodos\"\r\n                :key=\"index+1\"\r\n                :data=\"item\"\r\n                v-for=\"(item, index) in Buscatodos32\"/>\r\n              </div>\r\n              <!-- Aqui começa com 64 itens-->\r\n              <div class=\"row\" v-show=\"mostra64\">\r\n                <card-product\r\n                v-show=\"mostratodos\"\r\n                :key=\"index+2\"\r\n                :data=\"item\"\r\n                v-for=\"(item, index) in Buscatodos64\"/>\r\n              </div>\r\n              <div class=\"justify-content-center\">\r\n                <button @click=\"mostraMais\" class=\"btn btn-outline-primary\">Mostrar Mais</button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport { db } from '../firebase'\r\nimport ModalProduct from '../components/market/ModalProduct'\r\n/* import NavLeft from '../components/market/NavLeft.vue' */\r\nimport CardProduct from '../components/market/CardProduct.vue'\r\n\r\nexport default {\r\n  name: 'Market',\r\n  components: {\r\n    /* NavLeft, */\r\n    CardProduct,\r\n    ModalProduct\r\n  },\r\n  data () {\r\n    return {\r\n      indicacoes16: [],\r\n      indicacoes32: [],\r\n      indicacoes64: [],\r\n      mostratodos: true,\r\n      imo: false,\r\n      pro: false,\r\n      ser: false,\r\n      vei: false,\r\n      dig: false,\r\n      emp: false,\r\n      ap: false,\r\n      tod: true,\r\n      mostra16: true,\r\n      mostra32: false,\r\n      mostra64: false,\r\n      busca: '',\r\n      cont: 0\r\n    }\r\n  },\r\n  computed: {\r\n    Buscatodos16: function () {\r\n      if (this.busca === '' || this.busca === ' ' || this.busca === null) {\r\n        return this.indicacoes16\r\n      } else {\r\n        return this.indicacoes16\r\n          .filter(index => {\r\n            return (\r\n              this.busca.toLowerCase().split(' ').every(v =>\r\n                index.titulo.toLowerCase().includes(v) ||\r\n                index.empresa.toLowerCase().includes(v)\r\n              )\r\n            )\r\n          })\r\n      }\r\n    },\r\n    Buscatodos32: function () {\r\n      if (this.busca === '' || this.busca === ' ' || this.busca === null) {\r\n        return this.indicacoes32\r\n      } else {\r\n        return this.indicacoes32\r\n          .filter(index => {\r\n            return (\r\n              this.busca.toLowerCase().split(' ').every(v =>\r\n                index.titulo.toLowerCase().includes(v) ||\r\n                index.empresa.toLowerCase().includes(v)\r\n              )\r\n            )\r\n          })\r\n      }\r\n    },\r\n    Buscatodos64: function () {\r\n      if (this.busca === '' || this.busca === ' ' || this.busca === null) {\r\n        return this.indicacoes64\r\n      } else {\r\n        return this.indicacoes64\r\n          .filter(index => {\r\n            return (\r\n              this.busca.toLowerCase().split(' ').every(v =>\r\n                index.titulo.toLowerCase().includes(v) ||\r\n                index.empresa.toLowerCase().includes(v)\r\n              )\r\n            )\r\n          })\r\n      }\r\n    }\r\n  },\r\n  created () {\r\n    this.$root.$emit('MenuBar::show')\r\n    this.$root.$emit('marketset:show')\r\n    this.$root.$emit('indset:hide')\r\n    this.getData()\r\n  },\r\n  methods: {\r\n    mostraMais () {\r\n      this.cont = this.cont + 1\r\n      if (this.mostra16 === true) {\r\n        this.mostra16 = false\r\n        this.getData32()\r\n        this.mostra32 = true\r\n        this.cont = this.cont + 1\r\n      } if (this.motra32 === true && this.cont === 3) {\r\n        this.mostra32 = false\r\n        this.mostra64 = true\r\n        this.getData64()\r\n        this.cont = this.cont + 1\r\n      }\r\n    },\r\n    getData () {\r\n      const reft = db.collection('mercado').orderBy('valor').limit(16).get()\r\n      reft.then(snapshot => {\r\n        snapshot.forEach(doc => {\r\n          this.indicacoes16.push(doc.data())\r\n        })\r\n      })\r\n    },\r\n    getData32 () {\r\n      const reft = db.collection('mercado').orderBy('valor').limit(32).get()\r\n      reft.then(snapshot => {\r\n        snapshot.forEach(doc => {\r\n          this.indicacoes32.push(doc.data())\r\n        })\r\n      })\r\n    },\r\n    getData64 () {\r\n      const reft = db.collection('mercado').orderBy('valor').limit(64).get()\r\n      reft.then(snapshot => {\r\n        snapshot.forEach(doc => {\r\n          this.indicacoes32.push(doc.data())\r\n        })\r\n      })\r\n    },\r\n    imoveisb () {\r\n      this.mostratodos = false\r\n      this.tod = false\r\n      this.imo = true\r\n      this.pro = false\r\n      this.ser = false\r\n      this.dig = false\r\n      this.emp = false\r\n      this.ap = false\r\n      this.vei = false\r\n      this.$root.$emit('imoveis')\r\n    },\r\n    veiculosb () {\r\n      this.mostratodos = false\r\n      this.vei = true\r\n      this.tod = false\r\n      this.imo = false\r\n      this.pro = false\r\n      this.ser = false\r\n      this.dig = false\r\n      this.emp = false\r\n      this.ap = false\r\n      this.$root.$emit('veiculos')\r\n    },\r\n    prodfisicob () {\r\n      this.mostratodos = false\r\n      this.vei = false\r\n      this.tod = false\r\n      this.imo = false\r\n      this.pro = true\r\n      this.ser = false\r\n      this.dig = false\r\n      this.emp = false\r\n      this.ap = false\r\n      this.$root.$emit('prodfisico')\r\n    },\r\n    digitaisb () {\r\n      this.mostratodos = false\r\n      this.vei = false\r\n      this.tod = false\r\n      this.imo = false\r\n      this.pro = false\r\n      this.ser = false\r\n      this.dig = true\r\n      this.emp = false\r\n      this.ap = false\r\n      this.$root.$emit('digital')\r\n    },\r\n    servfisicob () {\r\n      this.mostratodos = false\r\n      this.vei = false\r\n      this.tod = false\r\n      this.imo = false\r\n      this.pro = false\r\n      this.ser = true\r\n      this.dig = false\r\n      this.emp = false\r\n      this.ap = false\r\n      this.$root.$emit('servfisico')\r\n    },\r\n    empresab () {\r\n      this.mostratodos = false\r\n      this.vei = false\r\n      this.tod = false\r\n      this.imo = false\r\n      this.pro = false\r\n      this.ser = false\r\n      this.dig = false\r\n      this.emp = true\r\n      this.ap = false\r\n      this.$root.$emit('empresa')\r\n    },\r\n    appb () {\r\n      this.mostratodos = false\r\n      this.vei = false\r\n      this.tod = false\r\n      this.imo = false\r\n      this.pro = false\r\n      this.ser = false\r\n      this.dig = false\r\n      this.emp = false\r\n      this.ap = true\r\n      this.$root.$emit('app')\r\n    },\r\n    todosb () {\r\n      this.mostratodos = true\r\n      this.vei = false\r\n      this.tod = true\r\n      this.imo = false\r\n      this.pro = false\r\n      this.ser = false\r\n      this.dig = false\r\n      this.emp = false\r\n      this.ap = false\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.txt-customer{\r\n  font-size:12px!important;\r\n  font-family: 'Avenir', 'Poppins', sans-serif;\r\n}\r\n.btn-circle {\r\n      width: 30px;\r\n      height: 30px;\r\n      text-align: center;\r\n      padding: 6px 0;\r\n      font-size: 12px;\r\n      line-height: 1.428571429;\r\n      border-radius: 15px;\r\n    }\r\n\r\n    .btn-circle.btn-lg {\r\n      width: 50px;\r\n      height: 50px;\r\n      padding: 10px 16px;\r\n      font-size: 18px;\r\n      line-height: 1.33;\r\n      border-radius: 25px;\r\n    }\r\n\r\n    .btn-circle.btn-xl {\r\n      width: 70px;\r\n      height: 70px;\r\n      padding: 10px 16px;\r\n      font-size: 24px;\r\n      line-height: 1.33;\r\n      border-radius: 35px;\r\n    }\r\n</style>\r\n"]}]}