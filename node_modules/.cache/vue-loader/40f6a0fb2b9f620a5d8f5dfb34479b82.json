     <div class=\"col-md-7 pr-5 pt-2\" style=\"\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-12 text-right\"><span class=\"badge badge-pill text-right badge-primary\">Indicação via <span>{{tipo}}</span></span></div>\r\n              </div>\r\n              <h5 class=\"text-right mb-0 pt-1\">{{titulo}}</h5>\r\n              <p class=\"text-right text-info\">{{empresa}}</p>\r\n              <div class=\"row mt-5 pt-3\">\r\n                <div class=\"mt-2 col-md-8\">\r\n                  <p class=\"text-right mb-0\" style=\"font-size:15px;margin-top:3px\">Comissão por <span>{{comissao}}</span></p>\r\n                  <p class=\"lead text-right py-0 my-0\" style=\"font-weight:900; font-size:26px;line-height:25px\" v-money-format=\"valor\"></p>\r\n                </div>\r\n                <div class=\"text-right py-1 mt-2 col-md-4\"><button type=\"button\" class=\"btn btn-dark btn-block btn-lg\" href=\"#\" @click=\"indicar()\" data-dismiss=\"modal\">Indicar</button></div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"card text-center mt-3\">\r\n            <div class=\"card-header pt-0\">\r\n              <ul class=\"nav nav-tabs justify-content-center card-header-tabs\">\r\n                <li class=\"nav-item\">\r\n                  <a class=\"nav-link text-dark active\" data-toggle=\"tab\" data-target=\"#tabone\" href=\"\">Sobre</a>\r\n                </li>\r\n                <li class=\"nav-item\">\r\n                  <a class=\"nav-link text-dark\" href=\"\" data-toggle=\"tab\" data-target=\"#tabtwo\">Comissões</a>\r\n                </li>\r\n                <li class=\"nav-item\">\r\n                  <a class=\"nav-link text-dark\" data-toggle=\"tab\" data-target=\"#tabthree\" href=\"\">Termos</a>\r\n                </li>\r\n              </ul>\r\n            </div>\r\n            <div class=\"card-body px-5\">\r\n              <div class=\"tab-content mt-0\">\r\n                <div class=\"tab-pane fade active show\" id=\"tabone\" role=\"tabpanel\">\r\n                  <p class=\"\">{{sobre}}</p>\r\n                </div>\r\n                <div class=\"tab-pane fade\" id=\"tabtwo\" role=\"tabpanel\">\r\n                  <div class=\"table-responsive\">\r\n                    <table class=\"table table-striped mx-auto mb-0\">\r\n                      <thead class=\"text-center text-white table-muted table-dark\">\r\n                        <tr>\r\n                          <th scope=\"col\">Tipo</th>\r\n                          <th scope=\"col\" class=\"\">Valor</th>\r\n                        </tr>\r\n                      </thead>\r\n                      <tbody class=\"text-center\">\r\n                        <tr>\r\n                          <td>Comissão Venda Concretizada</td>\r\n                          <td>R$ 100,00</td>\r\n                        </tr>\r\n                        <tr>\r\n                          <td>Comissão Leads</td>\r\n                          <td>R$ 0,50</td>\r\n                        </tr>\r\n                        <tr class=\"\">\r\n                          <td>Comissão após aluguel</td>\r\n                          <td>R$ 200,00</td>\r\n                        </tr>\r\n                      </tbody>\r\n                    </table>\r\n                  </div>\r\n                </div>\r\n                <div class=\"tab-pane fade\" id=\"tabthree\" role=\"tabpanel\">\r\n                  <p class=\"\">{{termo}}</p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { auth, db } from '../../firebase'\r\nexport default {\r\n  data () {\r\n    return {\r\n      id: '',\r\n      titulo: '',\r\n      sobre: '',\r\n      valor: '',\r\n      tipo: '',\r\n      empresa: '',\r\n      comissao: '',\r\n      termo: '',\r\n      img: '',\r\n      useruid: '',\r\n      ver: true\r\n    }\r\n  },\r\n  created () {\r\n    this.$root.$on('showid', payload => {\r\n      this.id = payload.date\r\n      this.titulo = payload.title\r\n      this.valor = payload.value\r\n      this.sobre = payload.sob\r\n      this.tipo = payload.type\r\n      this.empresa = payload.business\r\n      this.comissao = payload.comissa\r\n      this.termo = payload.terms\r\n      this.img = payload.imagem\r\n      this.fisic = payload.fisico\r\n    })\r\n  },\r\n  methods: {\r\n    indicar () {\r\n      var user = auth.currentUser\r\n      if (user != null) {\r\n        this.useruid = user.uid\r\n      }\r\n      db.collection('IndicacoesUsuarios').doc('Uid').collection(this.useruid).doc(this.id).set({\r\n        id: this.id,\r\n        titulo: this.titulo,\r\n        valor: this.valor,\r\n        sobre: this.sobre,\r\n        tipo: this.tipo,\r\n        empresa: this.empresa,\r\n        comissao: this.comissao,\r\n        termo: this.termo,\r\n        img: this.img,\r\n        fisico: this.fisic\r\n      }).then(() => {\r\n        this.$router.push({ name: 'MyIndications' })\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]}]}