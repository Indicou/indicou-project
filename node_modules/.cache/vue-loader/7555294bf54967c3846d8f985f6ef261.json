 'none'}\" >\r\n    <div class=\"modal-dialog modal-dialog-centered\" role=\"document\">\r\n    <div class=\"modal-content\">\r\n      <div class=\"modal-header\">\r\n        <h5 class=\"modal-title\" id=\"exampleModalLongTitle\">Cadastro</h5>\r\n        <button type=\"button\" class=\"close\" @click=\"closeModal()\">\r\n          <span aria-hidden=\"true\">&times;</span>\r\n        </button>\r\n      </div>\r\n      <div class=\"modal-body\">\r\n      <div class=\"card-body\">\r\n        <div class=\"form-group\">\r\n          <input\r\n          required\r\n          type=\"text\"\r\n          v-model.trim=\"userData.userid\"\r\n          class=\"form-control\"\r\n          placeholder=\"Como quer ser chamado?\">\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <input\r\n          required\r\n          type=\"email\"\r\n          v-model.trim=\"userData.email\"\r\n          class=\"form-control\"\r\n          placeholder=\"E-mail\">\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <input\r\n          required\r\n          type=\"password\"\r\n          v-model.trim=\"userData.password\"\r\n          class=\"form-control\"\r\n          placeholder=\"Senha\">\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <input\r\n          required\r\n          type=\"password\"\r\n          v-model.trim=\"userData.password2\"\r\n          class=\"form-control\"\r\n          placeholder=\"Confirme sua Senha\">\r\n        </div>\r\n      </div>\r\n      <div>\r\n        <button class=\"btn btn-danger w-100 custom-button\" :disabled=\"loading\">\r\n          <template v-if=\"loading\">\r\n            Cadastrando... <i class=\"fas fa-spinner fa-spin\"></i>\r\n          </template>\r\n          <template v-else>\r\n            Confirmar <i class=\"fas fa-sign-in-alt\"></i>\r\n          </template>\r\n        </button>\r\n      </div>\r\n      </div>\r\n    </div>\r\n    </div>\r\n  </div>\r\n    </form>\r\n  </div>\r\n  </transition>\r\n</template>\r\n\r\n<script>\r\nimport { db, auth } from '../../firebase'\r\nexport default {\r\n  name: 'Register',\r\n  data () {\r\n    return {\r\n      userData: {\r\n        userid: '',\r\n        email: '',\r\n        password: '',\r\n        password2: ''\r\n      },\r\n      showRegisterModal: true,\r\n      loading: false,\r\n      useruid: ''\r\n    }\r\n  },\r\n  created () {\r\n    this.$root.$on('ModalRegister::show', () => {\r\n      this.showRegisterModal = true\r\n    })\r\n  },\r\n  methods: {\r\n    closeModal () {\r\n      this.showRegisterModal = false\r\n    },\r\n    async registerUser () {\r\n      this.loading = true\r\n      try {\r\n        auth.createUserWithEmailAndPassword(this.userData.email, this.userData.password)\r\n          .then(() => {\r\n            var user = auth.currentUser\r\n            if (user != null) {\r\n              this.useruid = user.uid\r\n            }\r\n            auth.currentUser.sendEmailVerification()\r\n            auth.currentUser.updateProfile({\r\n              displayName: this.userData.userid\r\n            }).then(() => {\r\n              db.collection('users').doc(this.useruid).set({\r\n                email: this.userData.email,\r\n                userid: this.userData.userid\r\n              }).then(() => {\r\n                this.$router.replace('MyIndications')\r\n              })\r\n            })\r\n          })\r\n      } catch (err) {\r\n        console.error(err)\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.fundo{\r\n  background-color: rgba($color: #000000, $alpha: 0.3);\r\n}\r\n</style>\r\n"]}]}